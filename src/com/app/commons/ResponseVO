package com.app.commons;


public class ResponseVO {
	
	private boolean anyException;
	private Exception exception;
	private int insertionCount;
	
	public boolean isAnyException() {
		return anyException;
	}
	public void setAnyException(boolean anyException) {
		this.anyException = anyException;
	}
	public Exception getExceptionObj() {
		return exception;
	}
	public void setExceptionObj(Exception exception) {
		this.exception = exception;
	}
	public int getInsertionCount() {
		return insertionCount;
	}
	public void setInsertionCount(int insertionCount) {
		this.insertionCount = insertionCount;
	}
	@Override
	public String toString() {
		return "ResponseVO [anyException=" + anyException + ", exceptionObj="
				+ exception + ", insertionCount=" + insertionCount + "]";
	}

	
}
